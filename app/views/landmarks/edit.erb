<form method="post" action="/landmarks/<%= @landmark.id %>/edit">
  <input id="hidden" type="hidden" name="_method" value="patch">

  <label>Edit Landmark:</label>

  <br></br>

  <input id="name" type="text" value="<%= @landmark.name %>" name="landmark[name]">

  <br></br>

  <label>Edit Year Completed:</label>

  <br></br>

  <input id="year_completed" type="text" value="<%= @landmark.year_completed %>" name="landmark[year_completed]">

  <br></br>

  <input type="submit" value="Edit Landmark">
</form>
